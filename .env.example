# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Bluesky OAuth
BLUESKY_CLIENT_ID=https://your-domain.com/client-metadata.json
BLUESKY_REDIRECT_URI=https://your-project.supabase.co/functions/v1/oauth-bluesky/callback

# Strava API
STRAVA_CLIENT_ID=your-strava-client-id
STRAVA_CLIENT_SECRET=your-strava-client-secret
STRAVA_WEBHOOK_VERIFY_TOKEN=your-random-verify-token
STRAVA_REDIRECT_URI=https://your-project.supabase.co/functions/v1/oauth-strava/callback

# Instagram/Facebook API
INSTAGRAM_APP_ID=your-instagram-app-id
INSTAGRAM_APP_SECRET=your-instagram-app-secret
INSTAGRAM_REDIRECT_URI=https://your-project.supabase.co/functions/v1/oauth-instagram/callback

# Application Configuration
NODE_ENV=development
APP_URL=http://localhost:3000

# Security
ENCRYPTION_KEY=your-32-character-encryption-key-here

# Rate Limiting (requests per minute)
RATE_LIMIT_PER_USER=60
RATE_LIMIT_PER_API_KEY=100
